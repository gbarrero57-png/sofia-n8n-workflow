# ── Redis ──────────────────────────────────────────────────
REDIS_PASSWORD=change_me_strong_password_here

# ── n8n versión ────────────────────────────────────────────
N8N_VERSION=1.30.1

# ── n8n DB (usar PostgreSQL en producción) ─────────────────
N8N_DB_TYPE=postgresdb
N8N_PGHOST=your-postgres-host
N8N_PGPORT=5432
N8N_PGDB=n8n_sofia
N8N_PGUSER=n8n_user
N8N_PGPASSWORD=change_me_postgres_password

# ── n8n Admin ──────────────────────────────────────────────
N8N_ADMIN_USER=admin
N8N_ADMIN_PASSWORD=change_me_strong_admin_password
N8N_ENCRYPTION_KEY=generate_with_openssl_rand_hex_32

# ── URLs ───────────────────────────────────────────────────
N8N_WEBHOOK_URL=https://workflows.n8n.redsolucionesti.com
N8N_EDITOR_URL=https://workflows.n8n.redsolucionesti.com

# ── SofIA Application ──────────────────────────────────────
N8N_SUPABASE_URL=https://inhyrrjidhzrbqecnptn.supabase.co
N8N_SUPABASE_SERVICE_KEY=your_rotated_service_role_key_here
N8N_CHATWOOT_WEBHOOK_TOKEN=your_chatwoot_webhook_token_here
N8N_ALERT_WEBHOOK_URL=https://hooks.slack.com/services/your/slack/webhook
N8N_WEBHOOK_MAX_AGE_SECONDS=300
